#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1

%:
	+dh $@ --parallel

override_dh_auto_configure:
	dh_auto_configure --sourcedirectory=wangle/ --

override_dh_auto_build:
	dh_auto_build --sourcedirectory=wangle/ --

override_dh_auto_clean:
	dh_auto_clean --sourcedirectory=wangle/ --

override_dh_auto_install:
	dh_auto_install --sourcedirectory=wangle/ --

override_dh_auto_test:
	dh_auto_test --sourcedirectory=wangle/ --
